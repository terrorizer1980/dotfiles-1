{
  "breadcrumbs.enabled": false,
  "csharp.format.enable": false,
  "csharp.referencesCodeLens.enabled": false,
  "css.validate": false,
  "editor.cursorBlinking": "phase",
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.renderIndentGuides": false,
  "editor.tabSize": 2,
  "elixirLS.fetchDeps": false,
  "elixirLS.suggestSpecs": false,
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "gitignore": "gitignore",
    "pryrc": "ruby",
    "spacemacs": "lisp",
    "vimrc": "viml",
    "zlogin": "shellscript",
    "zlogout": "shellscript",
    "zpreztorc": "shellscript",
    "zshenv": "shellscript",
    "zshrc": "shellscript",
    "*.eex": "HTML (Eex)"
  },
  "javascript.format.enable": false,
  "ruby.format": "rubocop",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "tslint.jsEnable": false,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "[Monokai Pro (Filter Octagon)]": {
      "editorGroupHeader.tabsBackground": "#1d1f2a",
      "editorCursor.foreground": "#fff5",
      "tab.activeBorder": "#272a39",
      "tab.border": "#1d1f2a",
      "tab.inactiveBackground": "#1d1f2a",
      "tab.unfocusedActiveBorder": "#272a39",
      "list.inactiveFocusBackground": "#272a39"
    }
  },
  "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",
  "workbench.startupEditor": "none",
  "workbench.statusBar.feedback.visible": false,
  "workbench.sideBar.location": "left",
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.leader": ",",
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.sneak": true,
  "vim.easymotionMarkerFontFamily": "Fira Code",
  "vim.easymotionMarkerHeight": 18,
  "vim.easymotionMarkerWidthPerChar": 8,
  "vim.easymotionMarkerFontSize": "14",
  "vim.useSystemClipboard": true,
  "vim.foldfix": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["[", "<Space>"],
      "commands": ["editor.action.insertLineBefore"]
    },
    {
      "before": ["]", "<Space>"],
      "commands": ["editor.action.insertLineAfter"]
    },
    {
      "before": ["[", "e"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["]", "e"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["<leader>", "t", "f"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Test Current File"
        }
      ]
    },
    {
      "before": ["<leader>", "t", "n"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Run Nearest Test"
        }
      ]
    },
    {
      "before": ["<leader>", "d", "n"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Debug Nearest Test"
        }
      ]
    },
    {
      "before": ["<leader>", "t", "l"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Run Last Test"
        }
      ]
    },
    {
      "before": ["<leader>", "d", "l"],
      "commands": [
        {
          "command": "workbench.action.tasks.runTask",
          "args": "Debug Last Test"
        }
      ]
    }
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 0,
  "csharpfixformat.style.braces.onSameLine": false,
  "[csharp]": {
    "editor.tabSize": 4
  },
  "csharpfixformat.style.spaces.beforeParenthesis": false,
  "search.location": "sidebar",
  "workbench.list.keyboardNavigation": "filter",
  "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
  "extensions.ignoreRecommendations": false,
  "editor.fontSize": 13
}
