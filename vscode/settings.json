{
    "csharp.referencesCodeLens.enabled": false,
    "editor.cursorBlinking": "phase",
    "editor.fontFamily": "IBM Plex Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.formatOnSave": false,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": false,
    "editor.tabSize": 2,
    "eslint.packageManager": "yarn",
    "lua.format.indentCount": 2,
    "lua.format.lineWidth": 80,
    "lua.targetVersion": "5.3",
    "pixelbyte.love2d.path": "/usr/local/bin/love",
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "IBM Plex Mono",
    "vim.enableNeovim": true,
    "vim.leader": ",",
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.sneak": true,
    "window.zoomLevel": 0,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "statusBar.background": "#1a1a1a"
        },
        "[dark-plus-syntax]": {
            "statusBar.background": "#1a1a1a"
        },
        "[greenery]": {
            "statusBar.background": "#1a1a1a",
            "statusBar.foreground": "#aaaaaa",
            "editor.selectionBackground": "#2b2e2c",
            "sideBar.background": "#1a1a1a",
            "tab.activeBorder": "#1f1f1f",
            "tab.hoverBorder": "#3c3c3c",
            "tab.hoverBackground": "#3c3c3c"
        }
    },
    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            "comments": "#707070",
        },
        "[dark-plus-syntax]": {
            "textMateRules": [
                {
                    "scope": "comment",
                    "settings": {
                        "foreground": "#707070",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "comment.documentation.heredoc.elixir",
                    "settings": {
                        "foreground": "#707070",
                        "fontStyle": "italic"
                    }
                }
            ]
        },
        "[greenery]": {
            "comments": "#707070",
            "textMateRules": [
                {
                    "scope": "entity.name.type.module.elixir",
                    "settings": { "foreground": "#ff5370" }
                },
                {
                    "scope": "keyword.control.elixir",
                    "settings": { "foreground": "#c8dbbc" }
                },
                {
                    "scope": "constant.other.symbol.elixir",
                    "settings": { "foreground": "#76a4aa" }
                },
                {
                    "scope": "variable.other.readwrite.module.elixir",
                    "settings": { "foreground": "#83a06c" }
                },
                {
                    "scope": "variable.other.constant.elixir",
                    "settings": { "foreground": "#f07178", "fontStyle": "" }
                }
   
            ]
        },
    },
    "workbench.colorTheme": "greenery"
}