{
  "csharp.referencesCodeLens.enabled": false,
  "editor.cursorBlinking": "phase",
  "editor.fontFamily": "Hasklig",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.tabSize": 2,
  "elixirLS.fetchDeps": false,
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "gitignore": "gitignore",
    "pryrc": "ruby",
    "spacemacs": "lisp",
    "vimrc": "viml",
    "zlogin": "shellscript",
    "zlogout": "shellscript",
    "zpreztorc": "shellscript",
    "zshenv": "shellscript",
    "zshrc": "shellscript"
  },
  "javascript.format.enable": false,
  "ruby.format": "rubocop",
  "tslint.jsEnable": false,
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.leader": ",",
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.sneak": true,
  "workbench.colorCustomizations": {
    "[Monokai Pro (Filter Octagon)]": {
      "editorGroupHeader.tabsBackground": "#1d1f2a",
      "editorCursor.foreground": "#fff7",
      "tab.activeBorder": "#272a39",
      "tab.border": "#1d1f2a",
      "tab.inactiveBackground": "#1d1f2a",
      "tab.unfocusedActiveBorder": "#272a39"
    },
    "[Monokai Pro (Filter Spectrum)]": {
      "editorGroupHeader.tabsBackground": "#191919",
      "editorCursor.foreground": "#fff7",
      "tab.activeBorder": "#222",
      "tab.border": "#191919",
      "tab.inactiveBackground": "#191919",
      "tab.unfocusedActiveBorder": "#222"
    }
  },
  "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",
  "workbench.startupEditor": "newUntitledFile",
  "css.validate": false,
  "breadcrumbs.enabled": false,
  "workbench.statusBar.feedback.visible": false,
  "csharp.format.enable": false,
  "workbench.settings.editor": "json",
  "elixirLS.suggestSpecs": false,
  "workbench.sideBar.location": "left",
  "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
  "editor.renderIndentGuides": true
}
