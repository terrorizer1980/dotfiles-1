" vim-airline — Lean and mean status/tabline for Vim that‘s light as air.
" https://github.com/vim-airline/vim-airline

Plug 'vim-airline/vim-airline'

let g:airline_theme='hybrid'                 " Use hybrid airline theme
let g:airline#extensions#tabline#enabled = 1 " Use vim-airline tabline
let g:airline_powerline_fonts=0              " Don't render 'fancy' symbols

" vim:ft=vim

